nter\"\r\n            @click=\"register\"\r\n            >register</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\nexport default {\r\n  name: \"Register\",\r\n  data() {\r\n    return {\r\n      form: {},\r\n      rules: {\r\n        username: [\r\n          {\r\n            required: true,\r\n            message: \"请输入用户名\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        password: [\r\n          {\r\n            required: true,\r\n            min: 6,\r\n            max: 10,\r\n            message: \"Length should be 6 to 10\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        confirm: [\r\n          {\r\n            required: true,\r\n            min: 6,\r\n            max: 10,\r\n            message: \"Length should be 6 to 10\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    register() {\r\n      if (this.form.password != this.form.confirm) {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"两次密码不一致请重新输入\",\r\n        });\r\n        return;\r\n      } else {\r\n        //验证值是否通过在提交post请求\r\n        this.$refs[\"formRef\"].validate((valid) => {\r\n          if (valid) {\r\n            request.post(\"/user/register\", this.form).then((resultr) => {\r\n              if (resultr.code == \"0\") {\r\n                this.$message({\r\n                  type: \"success\",\r\n                  message: \"注册成功！\",\r\n                });\r\n                this.$router.push(\"/userLogin\");\r\n              } else {\r\n                this.$message({\r\n                  type: \"error\",\r\n                  message: resultr.msg,\r\n                });\r\n              }\r\n            });\r\n          }\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.global {\r\n  width: 100%;\r\n  height: 100vh;\r\n  background: rgb(113, 128, 216);\r\n  overflow: hidden;\r\n}\r\n\r\n.register {\r\n  width: 400px;\r\n  margin: 200px auto;\r\n}\r\n\r\n.head {\r\n  text-align: center;\r\n  font-size: 30px;\r\n  color: aliceblue;\r\n  margin: 20px auto;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":""}]}