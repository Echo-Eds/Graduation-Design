;AATT,oBAAc,IAAA,CAAA;AASL,GAbZ,E;AAOa,IAAA,MAAM,EAAA,QAAA,CACf;AAAA,aAGO,CAHP,mBAAA,CAGO,MAHP,EAAA,WAAA,EAGO,CAFL,YAAA,CAAkD,oBAAlD,EAAkD;AAAtC,QAAA,OAAK,EAAE,QAAA,CAAA;AAA+B,OAAlD,EAA8B;0BAAE;AAAA,iBAAM,C,WAAA,CAAN;AAAA,S,CAAF;;;;AAAA,OAA9B,E;;AAAA,Q,WAAA,CAEK,EADL,YAAA,CAAgE,oBAAhE,EAAgE;AAArD,QAAA,IAAI,EAAC,SAAgD;AAArC,QAAA,OAAK,EAAE,QAAA,CAAA;AAA8B,OAAhE,E;0BAA6C;AAAA,iBAAO,C,WAAA,CAAP;AAAA,S;;;;OAA7C,E;;AAAA,Q,WAAA,CACK,CAHP,CAGO,CAHP;AAAA,KADe,C;sBADjB;AAAA,aAAmB,CAAnB,WAAmB,CAAnB;AAAA,K;;;;GANF,E;;AAAA,I,8BAAA,C","sourcesContent":["<template>\r\n  <!-- 我的个人信息 -->\r\n  <div class=\"info\">\r\n    <div class=\"info_title\">\r\n      <span>UserCenter</span>\r\n      <br />\r\n      <span>个人中心</span>\r\n    </div>\r\n    <br />\r\n    <!-- <el-skeleton style=\"width: 240px\">\r\n      <template #template>\r\n        <el-skeleton-item variant=\"image\" style=\"width: 240px; height: 240px\" />\r\n        \r\n      </template>\r\n    </el-skeleton> -->\r\n    <div class=\"info_form\">\r\n      <!-- <div class=\"user_icon\">\r\n        <el-form :model=\"imgform\">\r\n          <el-form-item>\r\n            <el-dialog v-model=\"imgformdialogVisible\">\r\n              <span :v-model=\"imgform.id\"></span>\r\n            </el-dialog>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div> -->\r\n\r\n      <br /><br />\r\n      <el-form\r\n        :model=\"form\"\r\n        :label-position=\"right\"\r\n        label-width=\"auto\"\r\n        style=\"width: 640px\"\r\n        size=\"large\"\r\n      >\r\n        <el-form-item label=\"头像\">\r\n          <div class=\"user_icon\">\r\n            <el-upload\r\n              action=\"http://localhost:9090/files/upload\"\r\n              :on-success=\"fileUploadSuccess\"\r\n              :show-file-list=\"false\"\r\n            >\r\n              <img\r\n                v-if=\"imageUrl\"\r\n                :src=\"imageUrl\"\r\n                :v-model=\"form.userIcon\"\r\n                class=\"avatar\"\r\n              />\r\n              <el-icon\r\n                v-else\r\n                class=\"avatar-uploader-icon\"\r\n                style=\"width: 68px; height: 68px\"\r\n                ><Plus\r\n              /></el-icon>\r\n            </el-upload>\r\n          </div>\r\n         \r\n          <!-- <div class=\"user_icon\">\r\n            <el-button @click=\"uploadToDB\" >上传</el-button>\r\n          </div> -->\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"ID\">\r\n          <el-input prefix-icon=\"\" v-model=\"form.id\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户名\">\r\n          <el-input v-model=\"form.username\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"密码\">\r\n          <el-input v-model=\"form.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-select\r\n            v-model=\"form.sex\"\r\n            placeholder=\"请输入性别\"\r\n            style=\"width: 640px\"\r\n            >>\r\n            <el-option value=\"男\"></el-option>\r\n            <el-option value=\"女\"></el-option>\r\n            <el-option value=\"未知\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"昵称\">\r\n          <el-input placeholder=\"请输入昵称\" v-model=\"form.nickName\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\">\r\n          <el-input\r\n            type=\"textarea\"\r\n            placeholder=\"请设置地址\"\r\n            v-model=\"form.address\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button\r\n            type=\"primary\"\r\n            round\r\n            plain\r\n            style=\"width: 100%; text-align: center\"\r\n            @click=\"alterMyInfo\"\r\n            >修改信息</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n  <!-- 弹出确认框 -->\r\n  <el-dialog\r\n    v-model=\"dialogVisible\"\r\n    title=\"Tips\"\r\n    width=\"30%\"\r\n    :before-close=\"handleClose\"\r\n  >\r\n    <span>您还未登录！</span>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"backToIndex\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"goToLogin\">Confirm</el-button>\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\n\r\nexport default {\r\n  name: \"MyInfoCenter\",\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      imgformdialogVisible: false,\r\n      form: [],\r\n      imgform: [],\r\n      reloadForm: [],\r\n      imageUrl: \"\",\r\n      user_icon: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.load();\r\n  },\r\n  methods: {\r\n    load() {\r\n      let str = sessionStorage.getItem(\"user\" || \"{}\");\r\n      //如果存在用户信息就去登录 如果不存在就去\r\n      if (str) {\r\n        this.form = JSON.parse(str);\r\n        console.log(this.form);\r\n        this.imageUrl = this.form.userIcon;\r\n      } else {\r\n        this.dialogVisible = true;\r\n      }\r\n     \r\n    },\r\n\r\n    alterMyInfo() {\r\n      request.put(\"/myinfo/updatemyinfo\", this.form).then((result) => {\r\n        console.log(result);\r\n        if (result.code == \"0\") {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"修改成功\",\r\n          });\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg,\r\n          });\r\n        }\r\n        sessionStorage.clear;\r\n        sessionStorage.setItem(\"user\", JSON.stringify(result.data));\r\n        this.load();\r\n      });\r\n    },\r\n    // uploadToDB() {\r\n    //   console.log(this.imgform);\r\n    //   request.post(\"/myinfo/insert\", this.imgform).then((result) => {\r\n    //     console.log(result);\r\n    //   });\r\n    // },\r\n    goToLogin() {\r\n      this.$router.push(\"/userLogin\");\r\n    },\r\n    backToIndex() {\r\n      this.$router.push(\"/\");\r\n    },\r\n    fileUploadSuccess(res) {\r\n      console.log(res);\r\n      console.log(\"-------\");\r\n      console.log(this.imageUrl)\r\n      this.form.userIcon = res.data;\r\n      this.imageUrl = res.data;\r\n      console.log(\"-------\");\r\n      console.log(this.imageUrl)\r\n      console.log(this.form)\r\n      \r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.info {\r\n  width: 700px;\r\n  margin: 100px auto;\r\n  text-align: center;\r\n}\r\n.info span {\r\n  display: flex;\r\n  justify-content: center;\r\n  font-size: 24px;\r\n  font-weight: 400;\r\n}\r\n.user_icon {\r\n  width: 10%;\r\n  height: 70px;\r\n  margin: 10px 45%;\r\n}\r\n.user_icon img {\r\n  max-width: 70px;\r\n  max-height: 70px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}