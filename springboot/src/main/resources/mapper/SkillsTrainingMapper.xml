<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.demo.mapper.SkillsTrainingMapper">

    <select id="getType" resultType="com.springboot.demo.pojo.SkillsTraining">
        SELECT DISTINCT training_type  FROM `skills_training`;
    </select>
    <select id="getAll" resultType="com.springboot.demo.pojo.SkillsTraining">
        SELECT DISTINCT *  FROM `skills_training`;
    </select>
</mapper>