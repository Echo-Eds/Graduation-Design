<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.demo.mapper.MedicineResourceMapper">
    <select id="getAll" resultType="com.springboot.demo.pojo.MedicineResource">
        SELECT * FROM `medicine_resource`;
    </select>
    <select id="getType" resultType="com.springboot.demo.pojo.MedicineResource">
        SELECT DISTINCT type  FROM `medicine_resource`;
    </select>
</mapper>